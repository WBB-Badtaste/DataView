{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "program": "${workspaceRoot}/app/Main.js",
            "cwd": "${workspaceRoot}",
            "stopOnEntry": false, 
            "preLaunchTask": null, 
            "runtimeExecutable": "${workspaceRoot}/node_modules/.1.4.13@electron-prebuilt/dist/electron.exe", 
            //"runtimeExecutable": "${workspaceRoot}/node_modules/.1.4.13@electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron", 
            "runtimeArgs": [
                "--nolazy"
            ], 
            "env": {
                "NODE_ENV": "development"
            }, 
            "externalConsole": false, 
            "sourceMaps": false, 
            "outDir": null
        },
        {
            "type": "node",
            "request": "attach",
            "name": "附加到进程",
            "port": 5858,
            "address": "localhost", 
            "restart": false, 
            "sourceMaps": false, 
            "outDir": null, 
            "localRoot": "${workspaceRoot}", 
            "remoteRoot": null
        }
    ]
}